{
  "name": "n8n-nodes-transcribe-audio",
  "version": "0.1.21",
  "description": "Perform speech-to-text on audio files within your n8n workflows.This node provides local audio transcription, no internet or third-party APIs required for processing.",
  "keywords": [
    "n8n-community-node-package",
    "transcribe audio",
    "transcribe",
    "audio",
    "transcription",
    "subtitles",
    "captions",
    "speech-to-text",
    "asr",
    "huggingface",
    "transformers.js",
    "whisper",
    "ffmpeg",
    "mp3",
    "wav"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "",
  "author": {
    "name": "raisaroj",
    "email": "raisaroj360@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dioveath/n8n-nodes-transcribe-audio.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint -c .eslintrc.prepublish.js nodes package.json",
    "deploy": "tsc && gulp build:icons && docker compose up"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/AudioTranscribe/AudioTranscribe.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.1",
    "onnxruntime-web": "^1.22.0",
    "wavefile": "^11.0.0"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "npm:onnxruntime-web@^1.22.0"
    }
  }
}